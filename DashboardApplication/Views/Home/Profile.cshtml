@model UserToReturn

<div>
    <h1>Hello</h1>
    <br />
    <img src="~/images/@Model.Photo" alt="Alternate Text" />

    <br />
    <h1>@Model.FirstName</h1>
    <h1>You have successfully logged in @Model.Email</h1>

    <form method="post" asp-action="Logout" asp-controller="Home">
        <button type="submit">
            <a class="btn btn-outline-primary" style="border:2px solid">Logout</a>
        </button>
    </form>
</div>